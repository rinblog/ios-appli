target 'Prost' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Prost
  pod 'Firebase'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'MessageKit'
  pod 'JSQMessagesViewController'
  pod 'Firebase/Core'
  pod 'Firebase/Storage'

  target 'ProstTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ProstUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
